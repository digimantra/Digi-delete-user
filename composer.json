{
    "name": "Digi/account-deletion",
    "description": "A Laravel package for account deletion with OTP.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Digi\\AccountDeletion\\": "src/"
        }
    },
    "authors": [
        {
            "name": "php Developer",
            "email": "it@digimantra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
            "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "scripts": {
        "post-install-cmd": [
            "echo 'The Digi/AccountDeletion package has been installed successfully!'"
        ],
        "post-update-cmd": [
            "echo 'The Digi/AccountDeletion package has been updated successfully!'"
        ]
    },
    "extra": {
        "laravel":{
            "providers": [
                "Digi\\AccountDeletion\\AccountDeletionServiceProvider"
            ]
        }
    }
}